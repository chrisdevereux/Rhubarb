{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "object-literal-sort-keys": false,
    "member-access": false,
    "no-empty-interface": false,
    "interface-name": false
  },
  "linterOptions": {
    "exclude": [
      "config/**",
      "node_modules/**",
      "coverage/**"
    ]
  }
}