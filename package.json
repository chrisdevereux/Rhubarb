{
  "name": "rhubarb",
  "version": "0.1.0",
  "description": "A framework for rapidly building data-driven activism tools",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/config-conventional": "^7.1.2",
    "@types/lodash": "^4.14.116",
    "@types/webpack-dev-server": "^3.1.1",
    "commitlint": "^7.2.0",
    "husky": "^1.1.1",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "ts-jest": "^23.10.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.1"
  },
  "scripts": {
    "commitmsg": "commitlint -E GIT_PARAMS",
    "test": "jest",
    "start": "node examples/start-example",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.(js|ts|tsx|jsx)": [
      "prettier --write",
      "tslint --fix",
      "tsc --noemit",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^10.11.4",
    "@types/webpack": "^4.4.14",
    "clime": "^0.5.9",
    "core-js": "^2.5.7",
    "express": "^4.16.3",
    "image-webpack-loader": "^4.3.1",
    "lodash": "^4.17.11",
    "ts-loader": "^5.2.1",
    "tslib": "^1.9.3",
    "url-loader": "^1.1.1",
    "webpack": "^4.20.2",
    "webpack-dev-server": "^3.1.9"
  }
